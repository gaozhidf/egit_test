MAXLINE	client.cpp	8;"	d	file:
MAXLINE	server.cpp	7;"	d	file:
SERV_PORT	client.cpp	9;"	d	file:
SERV_PORT	server.cpp	8;"	d	file:
lookup_host	addrinfotest.cc	/^lookup_host (const char *host)$/;"	f	signature:(const char *host)
main	addrinfotest.cc	/^main (int argc, char *argv[])$/;"	f	signature:(int argc, char *argv[])
main	client.cpp	/^int main(int argc, char *argv[])$/;"	f	signature:(int argc, char *argv[])
main	server.cpp	/^int main(void)$/;"	f	signature:(void)
